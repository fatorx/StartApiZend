{
  "name": "StartAppZend3",
  "description": "Structure base Zend development",
  "homepage": "http://jhonmike.github.io/",
  "keywords": ["framework","zf3","skeleton", "zend3"],
  "license": "MIT",
  "authors": [
    {
      "name": "Jhon Mike Soares",
      "email": "developer@jhonmike.com.br",
      "homepage": "http://www.jhonmike.com.br",
      "role": "Developer"
    }
  ],
  "scripts": {
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php",
    "orm:create": "php vendor/bin/doctrine-module orm:schema-tool:create",
    "orm:drop": "php vendor/bin/doctrine-module orm:schema-tool:drop --force",
    "orm:fixture": "php vendor/bin/doctrine-module data-fixture:import",
    "phpunit": "php vendor/bin/phpunit",
    "test": "composer orm:drop \n composer orm:create \n composer phpunit"
  },
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-component-installer": "^0.1.0",
    "zendframework/zend-config": "^2.6",
    "zendframework/zend-mvc": "^3.0.0@dev",
    "doctrine/dbal": "2.5.*",
    "doctrine/orm": "2.5.*",
    "jhonmike/zf2-base": "dev-master",
    "jhonmike/zf2-acl": "dev-master",
    "jhonmike/zf2-user": "dev-master"
  },
  "require-dev": {
    "doctrine/data-fixtures": "1.1.*",
    "doctrine/migrations": "1.0.*@dev",
    "phpunit/phpunit": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "module/Application/src/"
    }
  }
}
