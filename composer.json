{
    "name": "jhonmike/StartApiZend",
    "description": "Skeleton Application for Zend Framework",
    "type": "project",
    "license": "BSD-3-Clause",
    "keywords": [
        "framework",
        "mvc",
        "zf2"
    ],
    "homepage": "http://framework.zend.com/",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/webimpress/DoctrineORMModule"
        },
        {
            "type": "vcs",
            "url": "https://github.com/webimpress/DoctrineModule"
        }
    ],
    "require": {
        "php": "^7.0",
        "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
        "zendframework/zend-mvc": "^3.0.2",
        "zendframework/zend-servicemanager": "^3.1.0",
        "zfcampus/zf-development-mode": "^3.0",
        "doctrine/doctrine-module": "dev-feature/zf3-support as 2.0",
        "doctrine/doctrine-orm-module": "dev-feature/zf3-support as 1.0"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/"
        }
    },
    "extra": [],
    "scripts": {
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "zendframework/zend-developer-tools": "^1.1"
    }
}
